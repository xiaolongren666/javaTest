package ink.xlr.test;

/**
 * @Author:小龙人
 * @File Name:Fruit.java
 * @Created Time:2019年3月21日上午11:04:38
 * @Introduce Function:TODO
 */
public class Fruit {
	private String name;
	private String color;
	private int weight;
	/** 品种 */
	private String variety;
	/** 采摘日期 */
	private String pickDate;
	/** 保质期 */
	private String expirationTime;
	

	
	// set get方法
	public String getColor() {
		return color;
	}

	public String getPickDate() {
		return pickDate;
	}

	public void setPickDate(String pickDate) {
		this.pickDate = pickDate;
	}

	public String getExpirationTime() {
		return expirationTime;
	}

	public void setExpirationTime(String expirationTime) {
		this.expirationTime = expirationTime;
	}

	public String getVariety() {
		return variety;
	}

	public void setVariety(String variety) {
		this.variety = variety;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}

	// 构造方法
	public Fruit() {
	}

	

	/**
	 * @param name
	 * @param color
	 * @param weight
	 * @param variety
	 * @param pickDate
	 * @param expirationTime
	 */
	public Fruit(String name, String color, int weight, String variety, String pickDate, String expirationTime) {
		super();
		this.name = name;
		this.color = color;
		this.weight = weight;
		this.variety = variety;
		this.pickDate = pickDate;
		this.expirationTime = expirationTime;
	}

	
	@Override
	public String toString() {
		return "Fruit [name=" + name + ", color=" + color + ", weight=" + weight + ", variety=" + variety
				+ ", pickDate=" + pickDate + ", expirationTime=" + expirationTime + "]";
	}
}

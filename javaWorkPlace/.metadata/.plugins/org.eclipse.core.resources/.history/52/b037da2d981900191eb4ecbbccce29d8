public class ArrayDemo {
	public static void main(String[] args) {
		int a = 10;
		
		int[] c = {1, 2, 3, 4, 5};
		
		int[] d = new int[]{1, 2, 3};
		
		int[] b = new int[10];
		b[0] = 1;
		b[1] = 2;
		b[2] = 3;
		
		System.out.println("b.length:" + b.length);
	}
}

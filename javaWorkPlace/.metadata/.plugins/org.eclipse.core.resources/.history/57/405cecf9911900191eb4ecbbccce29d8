public class IfDemo {
	public static void main(String[] args) {
		
		int grade = 67;
		
		if (i < 0) {
			System.out.println("附属");
		}
		
		if (g >= 90) {
			System.out.println("优秀");
		}
		if (g >= 80) {
			System.out.println("良好");
		}
		if (g >= 70) {
			System.out.println("中等");
		}
		if (g >= 60) {
			System.out.println("及格");
		}
		System.out.println("不及格");
	}
}

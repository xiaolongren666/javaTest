package ink.xlr.iframe;

import java.util.Random;
import java.util.Scanner;

/**
 * @Author:小龙人
 * @File Name:TwoOne.java
 * @Created Time:2019年3月14日下午2:28:55
 * @Introduce Function:TODO
 */
public class TwoOne {
	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner in =new Scanner(System.in);
		// 建立一个长度为10的数组用于存放数列中的数
		int[] arr = new int[10];
		int i = 1;
		// 先定义数列中的第一个和第二个数
		arr[0] = 1;
		arr[1] = 1;
		int max = in.nextInt();
		//		for (int i = 0; i < arr.length; i++) {
		while(true) {
			// 判断：当打印第三个数前，给第三个数赋值
			if (i > 1) {
				arr[i] = arr[i - 2] + arr[i - 1];
			}
			System.out.print(arr[i] + "\t");
			i++;
			if(i == max) {
				break;
			}
		}
	}

}

public class Test {
	public static void main(String[] args) {
		int k = 1, n = 10, m = 1;
		
		while(k <= n) {
			m *= 2;
			System.out.println("m="+m);
			n--;
			System.out.println("n="+n);
			
		}
		
		System.out.println("最后结果"+m);
		
		
	}
}

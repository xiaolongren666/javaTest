package xlr.chapter02.section05;

/*
*@Author:小龙人
*@File Name:ForDemo3.java
*@Created Time:下午9:57:03
*@Introduce Function:For
*/
public class ForDemo3 {
	public static void main(String[] args) {
		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <=i ; j++) {
				System.out.print(j + "*" + i + "=" + i * j + " ");
			}
			// 换行
			System.out.println();
		}
	}
}

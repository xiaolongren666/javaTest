public class Counter {
	// 静态变量，用于统计创建对象的个数
	public static int count = 0;
	public InstanceCounteDemo() {
		count++;
	}
	
	/**
	 * 静态方法，用于输出count的个数
	 */
	public static void printCount() {
		
	}
}

class Fruit {
	/** 重量 */
	private int weight;
	/** 颜色 */
	private String color;
	
	/**
	 * @param weight
	 * @param color
	 */
	public Fruit(int weight, String color) {
		super();
		this.weight = weight;
		this.color = color;
	}
}


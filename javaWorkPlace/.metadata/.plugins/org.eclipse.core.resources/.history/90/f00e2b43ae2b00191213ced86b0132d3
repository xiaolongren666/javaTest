public class Orange extends Fruit {

	/**
	 * @param weight
	 * @param color
	 */
	public Orange(int weight, String color) {
		super(weight, color);
		// TODO Auto-generated constructor stub
	}
	
	public void display() {
		System.out.println("桔子：重量：" + this.getWeight() + ",颜色:" + this.getColor());
	}
	
}

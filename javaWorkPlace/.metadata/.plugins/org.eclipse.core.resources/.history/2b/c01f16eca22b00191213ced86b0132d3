class Fruit {
	/** 重量 */
	private int weight;
	/** 颜色 */
	private String color;
	
	/**
	 * @param weight
	 * @param color
	 */
	public Fruit(int weight, String color) {
		super();
		this.weight = weight;
		this.color = color;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}
}

class Apple extends Fruit {
	
}

class Box {
	
}


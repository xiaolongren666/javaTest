package ink.xlr.test;

import java.util.Random;
import java.util.Set;

/**
*@Author:小龙人
*@File Name:WelfareLottery.java
*@Created Time:2019年3月21日下午1:42:59
*@Introduce Function:TODO
*/
public class WelfareLottery {
	Set<Integer> list = null;
	
	/**
	 * 随机生成五个1-23之间的数
	 * @return
	 */
	public Set<Integer> GenerateLottery() {
		while(list.size() == 5) {
			list.add(new Random().nextInt(22)+1);
		}
		
		for (Integer integer : list) {
			System.out.print(integer);
		}
		return list;
	}
	
	public static void main(String[] args) {
		WelfareLottery wl = new WelfareLottery()
	}
}
